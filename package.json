{
    "name": "aws-suretalk-2.0-backend",
    "version": "1.0.0",
    "description": "SureTalk 2.0 Backend API",
    "main": "server.js",
    "scripts": {
        "start": "node server.js",
        "dev": "nodemon server.js",
        "db:init": "node scripts/initDatabase.js",
        "db:migrate-limits": "node scripts/updateUserLimits.js",
        "db:add-notifications": "node scripts/addNotificationsTables.js",
        "db:migrate-Db": "node scripts/migrateDatabase.js",
        "db:reset": "node scripts/resetDatabase.js",
        "db:seed": "node scripts/seed.js",
        "setup": "npm run db:init && npm run db:seed",
        "docker:build": "docker build -t suretalk-backend .",
        "docker:run": "docker run -p 8080:8080 suretalk-backend"
    },
    "engines": {
        "node": ">=18.0.0"
    },
    "dependencies": {
        "archiver": "^7.0.1",
        "aws-sdk": "^2.1498.0",
        "axios": "^1.6.2",
        "bcrypt": "^5.1.1",
        "cors": "^2.8.5",
        "crypto": "^1.0.1",
        "date-fns": "^2.30.0",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "express-rate-limit": "^6.11.2",
        "express-slow-down": "^3.0.1",
        "express-validator": "^7.0.1",
        "helmet": "^7.2.0",
        "jsonwebtoken": "^9.0.2",
        "multer": "^1.4.5-lts.1",
        "multer-s3": "^3.0.1",
        "nodemailer": "^7.0.12",
        "pg": "^8.11.3",
        "stripe": "^14.15.0",
        "twilio": "^4.18.0",
        "uuid": "^9.0.1",
        "web-push": "^3.6.7",
        "winston": "^3.10.0",
        "ws": "^8.18.3"
    },
    "devDependencies": {
        "nodemon": "^3.0.1"
    }
}
